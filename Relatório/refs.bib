	File = {:Users/andre/Dropbox/FEUP - MIEIC/Disserta\c{c}\~{a}o/Bibliografia/Ferreira et al. - 2012 - A survey of current trends in smartphone based payment and validation services for public transport users.pdf:pdf},
	Keywords = {component,customer experience,innovation,mobile,payment,service science,supply chain,transport},
	Pages = {1--28},
	Title = {{A survey of current trends in smartphone based payment and validation services for public transport users}},
	Year = {2012},
